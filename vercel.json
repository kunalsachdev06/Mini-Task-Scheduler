{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/dashboard", "destination": "/frontend/dashboard-enhanced.html" },
    { "source": "/about", "destination": "/frontend/about.html" },
    { "source": "/history", "destination": "/frontend/history.html" },
    { "source": "/login", "destination": "/frontend/login.html" },
    { "source": "/register", "destination": "/frontend/register-enhanced.html" },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}