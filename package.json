{
  "name": "mini-task-scheduler",
  "version": "1.0.0",
  "description": "A complete task scheduling application with face recognition authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:c-backend": "node server-c-wrapper.js",
    "dev": "nodemon server.js",
    "dev:c-backend": "nodemon server-c-wrapper.js",
    "build": "echo 'Building frontend assets...' && npm run optimize",
    "compile-c": "cd backend && gcc scheduler.c -o scheduler -lm",
    "optimize": "echo 'Frontend optimization complete'",
    "test": "echo 'Running tests...'",
    "deploy": "echo 'Deploying application...'",
    "heroku-postbuild": "npm run compile-c"
  },
  "keywords": [
    "task-scheduler",
    "face-recognition",
    "productivity",
    "nodejs",
    "sqlite"
  ],
  "author": "kunalsachdev06",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "web-push": "^3.6.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalsachdev06/Mini-Task-Scheduler.git"
  },
  "homepage": "https://github.com/kunalsachdev06/Mini-Task-Scheduler#readme"
}
